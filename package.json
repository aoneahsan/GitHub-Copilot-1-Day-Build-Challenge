{
  "name": "reflectify",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run unlink-packages && vite",
    "serve": "npm run update:linked-packages && vite",
    "build": "npm run unlink-packages && vite build",
    "build:only": "vite build",
    "preview": "vite preview",
    "lint": "eslint",
    "cap:add-android": "cap add android",
    "cap:open-android": "cap open android",
    "cap:generate-assets:android": "capacitor-assets generate --android",
    "sync:apps-config": "trapeze run apps-config.yaml",
    "cap:copy-android": "cap copy android",
    "cap:sync": "npm run build && cap sync && npm run sync:apps-config",
    "update:linked-packages": "yarn install --force && yarn link zaions-tool-kit && yarn link zaions-react-tool-kit",
    "unlink-packages": "yarn unlink zaions-tool-kit && yarn unlink zaions-react-tool-kit && yarn install --force",
    "deploy": "npm run build && firebase deploy",
    "deploy:prod": "npm run build && firebase deploy --project production",
    "sync:local-packages": "npm run unlink-packages && npm run update:linked-packages",
    "install:publish-packages": "yarn add zaions-tool-kit zaions-react-tool-kit zaions-react-ui-kit",
    "uninstall:publish-packages": "yarn remove zaions-tool-kit zaions-react-tool-kit zaions-react-ui-kit",
    "cap:add-ios": "cap add ios",
    "cap:open-ios": "cap open ios",
    "cap:generate-assets:ios": "capacitor-assets generate --ios",
    "cap:copy-ios": "cap copy ios"
  },
  "dependencies": {
    "@capacitor/android": "^6.2.0",
    "@capacitor/app": "^6.0.2",
    "@capacitor/browser": "^6.0.5",
